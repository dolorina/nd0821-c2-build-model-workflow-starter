name: basic_cleaning
conda_env: conda.yml

entry_points:
  main:
    parameters:

      input_artifact:
        description: Name of the input artifact ## ADD DESCRIPTION
        type: string

      output_artifact:
        description: Name of the output artifact ## ADD DESCRIPTION
        type: string

      output_type:
        description: Type of the output artifact ## ADD DESCRIPTION
        type: string

      output_description:
        description: Description of the output artifact ## ADD DESCRIPTION
        type: string

      min_price: 
        description: Minimal rental price
        type: int

      max_price: 
        description: Maximal rental price
        type: int 


    command: >-
        python run.py  --input_artifact {input_artifact}  --output_artifact {output_artifact}  --output_type {output_type}  --output_description {output_description}  --min_price {min_price}  --max_price {max_price} 
